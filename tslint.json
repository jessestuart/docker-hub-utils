{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "defaultSeverity": "error",
  "linterOptions": {
    "include": ["src"]
  },
  "rules": {
    "semicolon": false,
    "no-console": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-namespace": false,
    "no-use-before-declare": false,
    "object-literal-sort-keys": true
  }
}
