    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@semantic-release/git": "7.0.16",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.137",
    "@types/md5": "2.1.33",
    "@types/node": "12.7.2",
    "@types/pino": "5.8.9",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-preset-minify": "0.5.1",
    "codecov": "3.5.0",
    "concurrently": "4.1.2",
    "dependency-cruiser": "5.0.1",
    "eslint": "6.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "jest-junit": "7.0.0",
    "md5": "2.2.1",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "semantic-release": "15.13.21",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/jessestuart/gatsby-source-docker-hub#readme",
  "keywords": [
    "api",
    "docker",
    "docker-hub",
    "typescript",
    "utils",
    "wrapper-api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessestuart/docker-hub-utils.git"
  },
  "scripts": {
    "build": "babel --extensions '.ts' src/ --out-dir dist/",
    "generate-types": "tsc -p tsconfig.json",
    "lint": "eslint --quiet src/ --ext ts,tsx",
    "prepublishOnly": "concurrently 'yarn build' && 'yarn ts:defs'",
    "rebuild": "rm -rf dist/* && concurrently 'yarn build' 'yarn generate-types'",
    "release": "yarn rebuild && semantic-release",
    "test": "concurrently 'yarn generate-types' 'yarn jest --coverage'"
  },
  "types": "index.d.ts"
}
