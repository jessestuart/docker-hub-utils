{
  "name": "docker-hub-utils",
  "description": "Typescript / Node utilities for interacting with the Docker Hub API.",
  "version": "1.10.25",
  "author": "Jesse Stuart <hi@jessestuart.com>",
  "dependencies": {
    "axios": "0.19.2",
    "camelcase-keys": "6.1.2",
    "luxon": "1.22.0",
    "pino": "5.16.0",
    "ramda": "0.27.0"
  },
  "devDependencies": {
    "@semantic-release/git": "7.0.18",
    "@types/axios": "0.14.0",
    "@types/jest": "24.0.23",
    "@types/luxon": "1.21.0",
    "@types/node": "12.12.8",
    "@types/pino": "5.8.13",
    "@types/ramda": "0.26.34",
    "@typescript-eslint/eslint-plugin": "canary",
    "@typescript-eslint/parser": "canary",
    "codecov": "3.6.5",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-junit": "9.0.0",
    "nodemon": "1.19.4",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "semantic-release": "15.14.0",
    "source-map-support": "0.5.16",
    "ts-jest": "25.2.0",
    "tsdx": "0.12.3",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/jessestuart/docker-hub-utils",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "api",
    "docker",
    "docker-hub",
    "typescript",
    "utils",
    "wrapper-api"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/docker-hub-utils.esm.js",
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessestuart/docker-hub-utils.git"
  },
  "scripts": {
    "build": "tsdx build",
    "ci": "JEST_JUNIT_OUTPUT=reports/junit/js-test-results.xml jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit",
    "docs": "typedoc --mode modules --out public",
    "lint": "eslint --quiet src/ --ext ts,tsx",
    "prepublishOnly": "yarn build",
    "push": "yarn build && yalc push",
    "rebuild": "rm -rf dist/* && yarn build",
    "release": "yarn rebuild && semantic-release",
    "start": "tsdx watch",
    "test": "yarn jest --cache --coverage"
  },
  "typings": "dist/index.d.ts"
}
